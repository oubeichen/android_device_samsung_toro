# add here entries already existing in main device fstab, but for which you want a blk_device2, fs_type2 or fs_options2
# used to partition sdcard and format it to ext2/ext3
# used also to stat for size of mtd/yaffs2 partitions
/dev/block/platform/omap/omap_hsmmc.0/by-name/system    /system f2fs defaults defaults
/dev/block/platform/omap/omap_hsmmc.0/by-name/userdata  /data f2fs defaults defaults
/dev/block/platform/omap/omap_hsmmc.0/by-name/cache     /cache  f2fs defaults defaults

